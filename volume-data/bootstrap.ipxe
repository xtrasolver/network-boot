#!ipxe
# Boot a persistent RancherOS to RAM

# Location of Kernel/Initrd images
set base-url http://192.168.14.100:8080

kernel ${base-url}/k3os-vmlinuz-amd64 k3os.mode=install k3os.install.iso_url=${base-url}/k3os-amd64.iso k3os.install.config_url=${base-url}/k3os-cloud-config k3os.install.silent=true k3os.install.device=/dev/sda
initrd ${base-url}/k3os-initrd-amd64
boot
